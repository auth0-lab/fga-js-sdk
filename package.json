{
  "name": "@auth0/fga",
  "version": "0.6.4",
  "description": "JavaScript and Node.js SDK for Auth0 Fine Grained Authorization (FGA)",
  "author": "Auth0Lab",
  "keywords": [
    "authorization",
    "fga",
    "fine-grained-authorization",
    "auth0-fga",
    "auth0-fine-grained-authorization",
    "auth0-fga-api-client",
    "sandcastle",
    "zanzibar"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/",
    "prepublishOnly": "rm -rf dist/ && npm run build",
    "test": "jest --config ./tests/jest.config.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:auth0-lab/fga-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/auth0-lab/fga-js-sdk/issues"
  },
  "homepage": "https://github.com/auth0-lab/fga-js-sdk#readme",
  "overrides": {
    "axios": {
      "follow-redirects": "1.14.8"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
