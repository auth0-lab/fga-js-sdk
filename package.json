{
  "name": "@auth0/fga",
  "version": "0.10.0",
  "description": "JavaScript and Node.js SDK for Okta Fine Grained Authorization (FGA)",
  "author": "Okta FGA",
  "keywords": [
    "okta-fga",
    "auth0-fga",
    "authorization",
    "fga",
    "fine-grained-authorization",
    "rebac",
    "zanzibar"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/",
    "prepublishOnly": "rm -rf dist/ && npm run build",
    "test": "npx jest --config ./tests/jest.config.js",
    "typecheck": "tsc  --skipLibCheck",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format:check": "prettier --check **/*.ts",
    "format:fix": "prettier --write **/*.ts"
  },
  "dependencies": {
    "@openfga/sdk": "^0.3.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "eslint-plugin-unused-imports": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:auth0-lab/fga-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/auth0-lab/fga-js-sdk/issues"
  },
  "homepage": "https://github.com/auth0-lab/fga-js-sdk#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
